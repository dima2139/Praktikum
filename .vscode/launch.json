                "--ei"   , "5",
                "--nee"  , "1",

                "--n"    , "256",
                "--gamma", "0.99",
                "--rsf"  , "1",

                "--ilr"  , "0.01",
                "--dcr"  , "1",
                "--dcs"  , "100",
                
                "--temp"
