        {
            "name": "proximalPolicyOptimization",
            "type": "python",
            "request": "launch",
            "program": "scripts/rl/stableBaselines3/proximalPolicyOptimization.py",
            "console": "integratedTerminal",
            "env": {"PYTHONPATH": "${workspaceRoot}"},
            "justMyCode": true
        },
    
        {
            "name": "trainAgent.py",
            "type": "python",
            "request": "launch",
            "program": "scripts/rl/trainAgent.py",
            "console": "integratedTerminal",
            "env": {"PYTHONPATH": "${workspaceRoot}"},
            "args": [
                "--i"    , "10000",
                "--j"    , "25",
                "--rbc"  , "1000",
                
                "--ics"  , "25",  // 125
